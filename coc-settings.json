  "python.jediEnabled": false,
  "python.venvFolders": [
    ".pyenv"
  ],
  "prettier.eslintIntegration": false,
  "prettier.singleQuote": true,
  "prettier.printWidth": 120,
  "prettier.proseWrap": "always",
